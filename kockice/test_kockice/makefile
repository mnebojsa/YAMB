all: kockice_tests.o runall.o unity.o unity_fixture.o

kockice_tests.o:
	gcc -c kockice_tests.c -o ../build/kockice_tests.o

runall.o:
	gcc -c runall.c -o ../build/runall.o

unity.o:
	gcc -c ../../unity/unity.c -o ../build/unity.o

unity_fixture.o:
	gcc -c ../../unity/unity_fixture.c -o ../build/unity_fixture.o
